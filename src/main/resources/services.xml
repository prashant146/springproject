<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student" class="com.one.Student" scope="prototype">
    </bean>

    <bean id="teacher" class="com.one.Teacher" scope="singleton">
    </bean>

    <bean id ="classWithStaticFactoryMethod" class="com.one.ClassWithStaticFactoryMethod" factory-method="getClassWithStaticFactoryMethod"/>

    <!-- more bean definitions go here -->

</beans>